{
    "name": "rehAppAngularJS",
    "version": "1.0.0",
    "description": "Management support system for the private rehabilitation cabinets - mobile application",
    "keywords": [
        "AngularJS",
        "angular",
        "johnpapa",
        "styleguide",
        "management",
        "support",
        "system",
        "rehabilitation",
        "cabinet"
    ],
    "homepage": "https://github.com/pamigomp/rehAppAngularJS",
    "bugs": {
        "url": "https://github.com/pamigomp/rehAppAngularJS/issues"
    },
    "license": "MIT",
    "author": "Michal Pietrzak",
    "repository": {
        "type": "git",
        "url": "https://github.com/pamigomp/rehAppAngularJS.git"
    },
    "scripts": {
        "postinstall": "bower install",
        "prestart": "npm install",
        "start": "http-server -a localhost -p 8000 -c-1",
        "pretest": "npm install",
        "test": "grunt karma:unit",
        "coveralls": "cat ./coverage/*/lcov.info | ./node_modules/.bin/coveralls && rm -rf ./coverage",
        "test-single-run": "grunt karma:singleRun",
        "preupdate-webdriver": "npm install",
        "update-webdriver": "webdriver-manager update",
        "preprotractor": "npm run update-webdriver",
        "protractor": "grunt protractor"
    },
    "devDependencies": {
        "bower": "^1.7.9",
        "coveralls": "^2.11.9",
        "grunt": "^1.0.1",
        "grunt-contrib-concat": "^1.0.1",
        "grunt-contrib-jshint": "^1.0.0",
        "grunt-contrib-uglify": "^1.0.1",
        "grunt-contrib-watch": "^1.0.0",
        "grunt-jscs": "^3.0.0",
        "grunt-karma": "^2.0.0",
        "grunt-protractor-runner": "^3.2.0",
        "http-server": "^0.9.0",
        "jasmine-core": "^2.4.1",
        "jasmine-reporters": "^2.1.1",
        "karma": "^0.13.22",
        "karma-chrome-launcher": "^1.0.1",
        "karma-coverage": "^1.0.0",
        "karma-firefox-launcher": "^1.0.0",
        "karma-jasmine": "^1.0.2",
        "karma-junit-reporter": "^1.0.0",
        "karma-phantomjs-launcher": "^1.0.0",
        "load-grunt-tasks": "^3.5.0",
        "phantomjs-prebuilt": "^2.1.7",
        "protractor": "^3.3.0",
        "selenium-webdriver": "^2.53.2"
    },
    "private": true
}